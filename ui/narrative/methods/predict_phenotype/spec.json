{
    "ver": "0.0.1",
    "authors": [
        "janakakbase"
    ],
    "contact": "https://kbase.us/contact-us/",
    "categories": ["active"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [
     {
          "id": "shock_id",
          "optional" : false,
          "advanced" : false,
          "allow_multiple" : false,
          "default_values" : [ "" ],
          "field_type" : "file",
          "text_options" : {
            "valid_ws_types" : [ ]
          }
        },
        {
        "id": "list_name",
        "optional": true,
        "advanced": false,
        "allow_multiple": false,
        "default_values": [ "" ],
        "field_type": "textarea",
        "textarea_options" : {
            "n_rows" : 15
            }
        },
    
        {
            "id": "classifier_name",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseClassifier.GenomeClassifier" ]
            }
        },
        {
           "id": "phenotype",
           "optional": false,
           "advanced": false,
           "allow_multiple": false,
           "default_values": [ "Gram type" ],
           "field_type": "dropdown",
           "dropdown_options": {
               "options": [
                   {
                     "value": "gram",
                     "display": "Gram type",
                     "id": "gram",
                     "ui_name": "Gram type"
                   },
                   {
                     "value": "resp",
                     "display": "Respiration type",
                     "id": "resp",
                     "ui_name": "Respiration type"
                   }
                ]
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name":"kb_genomeclassification",
            "method": "predict_phenotype",
            "input_mapping": [
                {
                    "input_parameter": "shock_id",
                    "target_property": "shock_id"

                },
                {
                    "input_parameter": "list_name",
                    "target_property": "list_name"

                },
                
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace"
                },
                {
                    "input_parameter": "classifier_name",
                    "target_property": "classifier_name"

                },
                {
                    "input_parameter": "phenotype",
                    "target_property": "phenotype"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                },
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "ws"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
